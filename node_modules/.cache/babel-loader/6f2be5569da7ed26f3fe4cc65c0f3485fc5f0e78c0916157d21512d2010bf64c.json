{"ast":null,"code":"import { mapActions, mapState } from \"vuex\";\nimport BackButton from \"../components/BackButton\";\nimport Navbar from \"../components/Navbar.vue\";\nimport BottomTab from \"../components/BottomTab.vue\";\nimport SearchModal from \"../components/SearchModal.vue\";\nexport default {\n  name: \"AppLayoutDefault\",\n  components: {\n    BackButton,\n    Navbar,\n    BottomTab,\n    SearchModal\n  },\n  computed: {\n    ...mapState(\"court\", [\"selectedCourt\"]),\n    backRoute() {\n      let to = null;\n      switch (this.$route.name) {\n        case \"court\":\n          to = {\n            name: \"home\"\n          };\n          break;\n        case \"courtDetails\":\n        case \"courtSchedule\":\n        case \"courtReport\":\n          to = {\n            name: \"home\"\n          };\n          break;\n        case \"legal\":\n          to = {\n            name: \"more\"\n          };\n          break;\n        case \"login\":\n        case \"register\":\n          to = {\n            name: \"home\"\n          };\n          break;\n      }\n      return to;\n    },\n    showSearch() {\n      return this.$route.name !== \"profile\";\n    },\n    isRainyView() {\n      return [\"court\", \"courtDetails\"].includes(this.$route.name) && this.selectedCourt?.isRaining;\n    },\n    hideBottomTab() {\n      return [\"login\", \"register\"].includes(this.$route.name);\n    }\n  },\n  mounted() {\n    this.getCourts();\n    this.getCourtAvailability();\n  },\n  methods: {\n    ...mapActions(\"court\", [\"getCourts\", \"getCourtAvailability\"])\n  }\n};","map":{"version":3,"names":["mapActions","mapState","BackButton","Navbar","BottomTab","SearchModal","name","components","computed","backRoute","to","$route","showSearch","isRainyView","includes","selectedCourt","isRaining","hideBottomTab","mounted","getCourts","getCourtAvailability","methods"],"sources":["src/layouts/AppLayoutDefault.vue"],"sourcesContent":["<template>\n  <div class=\"app-wrapper\" :class=\"{ rainy: isRainyView }\">\n    <Navbar>\n      <template v-slot:left>\n        <back-button v-if=\"backRoute\" :to=\"backRoute\" />\n      </template>\n      <template v-slot:right>\n        <button\n          v-if=\"showSearch\"\n          class=\"btn-search\"\n          @click=\"\n            () =>\n              $router.push({\n                name: 'profile',\n              })\n          \"\n        >\n          <font-awesome-icon icon=\"fa-solid fa-user\" />\n        </button>\n      </template>\n    </Navbar>\n    <div class=\"app-page\">\n      <slot />\n    </div>\n    <SearchModal />\n    <BottomTab v-if=\"!hideBottomTab\" />\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nimport BackButton from \"../components/BackButton\";\nimport Navbar from \"../components/Navbar.vue\";\nimport BottomTab from \"../components/BottomTab.vue\";\nimport SearchModal from \"../components/SearchModal.vue\";\n\nexport default {\n  name: \"AppLayoutDefault\",\n  components: { BackButton, Navbar, BottomTab, SearchModal },\n  computed: {\n    ...mapState(\"court\", [\"selectedCourt\"]),\n    backRoute() {\n      let to = null;\n      switch (this.$route.name) {\n        case \"court\":\n          to = { name: \"home\" };\n          break;\n        case \"courtDetails\":\n        case \"courtSchedule\":\n        case \"courtReport\":\n          to = { name: \"home\" };\n          break;\n        case \"legal\":\n          to = { name: \"more\" };\n          break;\n        case \"login\":\n        case \"register\":\n          to = { name: \"home\" };\n          break;\n      }\n      return to;\n    },\n    showSearch() {\n      return this.$route.name !== \"profile\";\n    },\n    isRainyView() {\n      return (\n        [\"court\", \"courtDetails\"].includes(this.$route.name) &&\n        this.selectedCourt?.isRaining\n      );\n    },\n    hideBottomTab() {\n      return [\"login\", \"register\"].includes(this.$route.name);\n    },\n  },\n  mounted() {\n    this.getCourts();\n    this.getCourtAvailability();\n  },\n  methods: {\n    ...mapActions(\"court\", [\"getCourts\", \"getCourtAvailability\"]),\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../scss/variable\";\n\n.app-wrapper {\n  width: 100%;\n  max-width: 400px;\n  margin: 0px auto;\n  height: 100vh;\n  position: relative;\n  overflow-y: auto;\n  overflow-x: hidden;\n  background: linear-gradient(0deg, #0e0036 0%, #000000 100%);\n\n  &.rainy {\n    background: linear-gradient(0deg, #000000 0%, #247591 100%);\n  }\n\n  .app-page {\n    height: calc(100vh - 136px);\n    overflow: scroll;\n    z-index: 2;\n\n    &::-webkit-scrollbar {\n      display: none;\n    }\n    & {\n      -ms-overflow-style: none;\n      scrollbar-width: none;\n    }\n  }\n\n  &::v-deep .adswrapper,\n  &::v-deep .adsins {\n    background-color: rgba(255, 255, 255, 0.22);\n    height: 80px;\n    width: 100%;\n  }\n\n  &::-webkit-scrollbar {\n    display: none;\n  }\n  & {\n    -ms-overflow-style: none;\n    scrollbar-width: none;\n  }\n  .btn-search {\n    background: none;\n    border: none;\n    color: $primary;\n    font-size: 1.3em;\n  }\n}\n</style>\n"],"mappings":"AA8BA,SAAAA,UAAA,EAAAC,QAAA;AACA,OAAAC,UAAA;AACA,OAAAC,MAAA;AACA,OAAAC,SAAA;AACA,OAAAC,WAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAL,UAAA;IAAAC,MAAA;IAAAC,SAAA;IAAAC;EAAA;EACAG,QAAA;IACA,GAAAP,QAAA;IACAQ,UAAA;MACA,IAAAC,EAAA;MACA,aAAAC,MAAA,CAAAL,IAAA;QACA;UACAI,EAAA;YAAAJ,IAAA;UAAA;UACA;QACA;QACA;QACA;UACAI,EAAA;YAAAJ,IAAA;UAAA;UACA;QACA;UACAI,EAAA;YAAAJ,IAAA;UAAA;UACA;QACA;QACA;UACAI,EAAA;YAAAJ,IAAA;UAAA;UACA;MACA;MACA,OAAAI,EAAA;IACA;IACAE,WAAA;MACA,YAAAD,MAAA,CAAAL,IAAA;IACA;IACAO,YAAA;MACA,OACA,0BAAAC,QAAA,MAAAH,MAAA,CAAAL,IAAA,KACA,KAAAS,aAAA,EAAAC,SAAA;IAEA;IACAC,cAAA;MACA,6BAAAH,QAAA,MAAAH,MAAA,CAAAL,IAAA;IACA;EACA;EACAY,QAAA;IACA,KAAAC,SAAA;IACA,KAAAC,oBAAA;EACA;EACAC,OAAA;IACA,GAAArB,UAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}